{
  "name": "case-github-search",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "lint:all:fix": "pnpm run type-check && pnpm run lint:fix && pnpm run format",
    "deps:check": "depcheck",
    "deps:update": "npm-check-updates -u",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "lodash": "^4.17.21",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-intersection-observer": "^9.16.0",
    "react-loading-indicators": "^1.0.1",
    "react-router": "^7.9.4",
    "scheduler": "^0.27.0",
    "tailwindcss": "^4.1.14",
    "use-context-selector": "^2.0.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.37.0",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.56.1",
    "@storybook/react-vite": "10.0.2",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/ui": "^4.0.5",
    "clsx": "^2.1.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.37.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-storybook": "10.0.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^16.2.3",
    "npm-check-updates": "^19.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-classnames": "^0.8.4",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "10.0.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9",
    "vitest": "^4.0.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "sh -c \"tsc --noEmit\"",
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "depcheck": {
    "ignores": [
      "lint-staged",
      "tailwindcss",
      "@testing-library/*",
      "@babel/*",
      "identity-obj-proxy",
      "eslint-import-resolver-typescript",
      "prettier-plugin-tailwindcss",
      "prettier-plugin-classnames",
      "prettier-plugin-merge",
      "scheduler"
    ],
    "skip-missing": true
  }
}
